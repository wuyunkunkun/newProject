<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<c:set var="ctx" value="${pageContext.request.contextPath}" />
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html lang="en" class="app">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
<title>教师列表</title>
<link rel="stylesheet" href="${ctx }/css/app.v2.css" type="text/css" />
<link rel="stylesheet" href="${ctx }/js/calendar/bootstrap_calendar.css" type="text/css" cache="false" />
<link rel="stylesheet" href="${ctx }/css/record_list.css" type="text/css" />

<!-- 教师列表js -->
<script type="text/javascript">
function sousuo(){
			var sousuo = document.getElementsByName("sousuo");
			var sousuoTypeValue = $("#sousuoType").find("option:selected").val();
			if(sousuoTypeValue == "caozuoleixing"){
				//按照操作的名字查找
				window.location.href="${ctx}/recorder/findRecorderByOperationType?operationName="+sousuo[0].value;
				
			}else if(sousuoTypeValue == "jiaoshigonghao"){
				//按照老师的id进行查找
				window.location.href="${ctx}/recorder/findRecorderByTeacher?t_id="+sousuo[0].value; 
			}else if(sousuoTypeValue == "laoshixingming"){
				//按照老师的姓名进行查找
				window.location.href="${ctx}/recorder/findRecorderByTeacherName?TeacherName="+sousuo[0].value; 
			}else{
				//没有条件查询
				window.location.href="${ctx}/recorder/findAllRecorder"; 
			}
}

function pageSkip(){
	var action="${choose}";
	if(action == "none"){
		var listpage = document.getElementsByName("nones");
		var listpageNum = document.getElementsByName("none");
		var pageNum = listpageNum[0].value;
		listpage[0].href = "${ctx }/recorder/findAllRecorder?pageNum="+pageNum;
	}else if(action == "t_id"){
		var editpage = document.getElementsByName("tids");
		var editpageNum = document.getElementsByName("tid");
		var pageNum = editpageNum[0].value;
		editpage[0].href = "${ctx }/recorder/findRecorderByTeacher?pageNum="+pageNum;
	}else if(action == "TeacherName"){
		var delpage = document.getElementsByName("teachernames");
		var delpageNum = document.getElementsByName("teachername");
		var pageNum = delpageNum[0].value;
		delpage[0].href = "${ctx }/recorder/findRecorderByTeacherName?pageNum="+pageNum;
	}else if(action == "operationType"){
		var operationtypes=document.getElementsByName("operationtypes");
		var operationtype=document.getElementsByName("operationtype");
		var pageNum = operationtype[0].value;
		operationtypes[0].href="${ctx }/recorder/findRecorderByOperationType?pageNum="+pageNum;
	}
}
</script>

</head>
<body>
<section class="vbox">
	<jsp:include page="../header.jsp"/>	
  	<section>
    <section class="hbox stretch"> <!-- .aside -->
    	<jsp:include page="../aside.jsp"/>
      	<!-- /.aside -->
      	<section id="content">
        	<section class="vbox">
          		<section class="scrollable padder">
          			 <div class="location">
          			 	组卷系统
          			 </div>
          	<form action="${ctx }/Teachermanage/list" method="post">
            		<div class="content">
            			<div class="blank10"></div>
            			<div class="search_block">
            				<div class="h">
            					<span class="icon-sprite icon-magnifier"></span>
            					<h3>快速搜索</h3>
        					</div>            			
            			 	<div class="sousuodiv">
            			 		<div class="sousuobutton">
            			 			<div class="yonghuming">
            			 				<div class="sousuo">
		<select id="sousuoType"> 
			<option>请选择搜索类型</option>
			<option value="jiaoshigonghao">按教师工号</option> 
			<option value="laoshixingming">按教师姓名</option> 
			<option value="caozuoleixing">按操作类型</option>
		</select>
		<!-- 按照老师的ID进行查寻 -->
		<c:if test="${choose == 't_id'}">
			<input name="sousuo" type="text" value="${t_id}"/>
		</c:if>
		<!-- 查找所有的记录 -->
		<c:if test="${choose == 'none'}">
			<input name="sousuo" type="text" value="${none}"/>
		</c:if>
		<!-- 按照老师的姓名进行查找 -->
		<c:if test="${choose == 'TeacherName' }">
			<input name="sousuo" type="text" value="${TeacherName}"/>
		</c:if>
		<!-- 按照操作查找所有的记录 -->
		<c:if test="${choose == 'operationType' }">
			<input name="sousuo" type="text" value="${operationName}"/>
		</c:if>
	</div>
            			 			</div>
            			 			<a id="sousuosubmit" onclick="sousuo()">搜索</a>
            			 		</div>
            			 	</div>
            			</div>
        			</div>
        			
        			<div class="blank10"></div>
        			
        			<div class="search_block">
        				<div class="h">
           					<span class="icon-sprite icon-list"></span>
            				<h3>修改试题记录列表</h3>
        				</div>
        				
        				<div class="cnt-wp">
        					<div class="cnt">
        						 <table class="data-table" width="100%" border="0" cellspacing="0" cellpadding="0">
        						 	 <tbody>
        						 	 	<tr>
											<td width="100px" height="60">老师工号</td>
											<td>老师姓名</td>
											<td>操作</td>
											<td>描述信息</td>
											<td>操作的时间 </td>
										</tr>
										<c:forEach items="${page.list}" var="recorder">
											<tr>
											<td>${recorder.teacher.id }</td>
											<td>${recorder.teacher.name}</td>
											<td>${recorder.operation.name}</td>
											<td>${recorder.description}</td>
											<td>${recorder.time }</td>
										</tr>
										</c:forEach>
        						 	 </tbody>
        						 </table>
        					</div>
        					<div class="dibu">
        						 <div class="sumpage">共    ${page.totalRecords}  条记录，每页    ${page.pageSize}  条记录</div>
        						 <div class="fenye">
        						 		<c:if test="${choose == 'operationType' }">
        						 				<a name="pagen" href="${ctx }/recorder/findRecorderByOperationType?pageNum=${page.topPageNo }&operationName=${operationName}"><span>首页</span></a>
										 		<a name="pagen" href="${ctx }/recorder/findRecorderByOperationType?pageNum=${page.previousPageNo }&operationName=${operationName}"><span>前一页</span></a>
												<a name="pagen" href="${ctx }/recorder/findRecorderByOperationType?pageNum=${page.nextPageNo }&operationName=${operationName}"><span>后一页</span></a>
												<a name="pagen" href="${ctx }/recorder/c?pageNum=${page.bottomPageNo }&operationName=${operationName}"><span>尾页</span></a>
	        						 			第<input type="text" value="${skipNum}" id="input_small" name="operationtype"/>页
        						 			    <a name="operationtypes" onclick="pageSkip()"><span>跳转</span></a>
        						 		</c:if>
        						 		<c:if test="${choose=='none'}">
        						 			<a name="pagen" href="${ctx }/recorder/findAllRecorder?pageNum=${page.topPageNo }"><span>首页</span></a>
									 		<a name="pagen" href="${ctx }/recorder/findAllRecorder?pageNum=${page.previousPageNo }"><span>前一页</span></a>
											<a name="pagen" href="${ctx }/recorder/findAllRecorder?pageNum=${page.nextPageNo }"><span>后一页</span></a>
											<a name="pagen" href="${ctx }/recorder/findAllRecorder?pageNum=${page.bottomPageNo }"><span>尾页</span></a>
        						 			第<input type="text" value="${skipNum}" id="input_small" name="none"/>页
        						 			<a name="nones" onclick="pageSkip()"><span>跳转</span></a>
        						 		</c:if>
        						 		<c:if test="${choose=='t_id'}">
        						 			<a name="pagen" href="${ctx }/recorder/findRecorderByTeacher?pageNum=${page.topPageNo }&t_id=${t_id}"><span>首页</span></a></li>
									 		<a name="pagen" href="${ctx }/recorder/findRecorderByTeacher?pageNum=${page.previousPageNo }&t_id=${t_id}"><span>前一页</span></a></li>
											<a name="pagen" href="${ctx }/recorder/findRecorderByTeacher?pageNum=${page.nextPageNo }&t_id=${t_id}"><span>后一页</span></a></li>
											<a name="pagen" href="${ctx }/recorder/findRecorderByTeacher?pageNum=${page.bottomPageNo }&t_id=${t_id}"><span>尾页</span></a></li>
        						 			第<input type="text" value="${skipNum}" id="input_small" name="tid"/>页
        						 			<a name="tids" onclick="pageSkip()"><span>跳转</span></a>
        						 		</c:if>
        						 		
        						 		<c:if test="${choose=='TeacherName'}">
						        	 		<a name="pagen" href="${ctx }recorder/findRecorderByTeacherName?pageNum=${page.topPageNo }&TeacherName=${TeacherName}"><span>首页</span></a></li>
									 		<a name="pagen" href="${ctx }/recorder/findRecorderByTeacherName?pageNum=${page.previousPageNo }&TeacherName=${TeacherName}"><span>前一页</span></a></li>
											<a name="pagen" href="${ctx }/recorder/findRecorderByTeacherName?pageNum=${page.nextPageNo }&TeacherName=${TeacherName}"><span>后一页</span></a></li>
											<a name="pagen" href="${ctx }/recorder/findRecorderByTeacherName?pageNum=${page.bottomPageNo }&TeacherName=${TeacherName}"><span>尾页</span></a></li>
        						 			第<input type="text" value="${skipNum}" id="input_small" name="teachername"/>页
        						 			<a name="teachernames" onclick="pageSkip()"><span>跳转</span></a>
        						 		</c:if>
        						 </div>
        					</div>
        				</div>
        			</div>
        		</form>
        		
            	</section>
        		</section>
        	</section>
        </section>
	</section>
</section>

<!-- 左边菜单 -->
<script src="${ctx }/js/app.v2.js"></script> <!-- Bootstrap --> <!-- App --> 
<script src="${ctx }/js/charts/easypiechart/jquery.easy-pie-chart.js" cache="false"></script> 
<script src="${ctx }/js/charts/sparkline/jquery.sparkline.min.js" cache="false"></script>
<script src="${ctx }/js/charts/flot/jquery.flot.min.js" cache="false"></script>
<script src="${ctx }/js/charts/flot/jquery.flot.tooltip.min.js" cache="false"></script> 
<script src="${ctx }/js/charts/flot/jquery.flot.resize.js" cache="false"></script> 
<script src="${ctx }/js/charts/flot/jquery.flot.grow.js" cache="false"></script> 
<script src="${ctx }/js/charts/flot/demo.js" cache="false"></script> 
<script src="${ctx }/js/calendar/bootstrap_calendar.js" cache="false"></script> 
<script src="${ctx }/js/calendar/demo.js" cache="false"></script> 
